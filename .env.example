# Developer Sentinel Configuration
# Copy this file to .env and customize as needed

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================

# Logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Polling interval in seconds (how often to check Jira for new issues)
POLL_INTERVAL_SECONDS=60

# Maximum retry attempts for agent execution on failure
MAX_RETRIES=3
